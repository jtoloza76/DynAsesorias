<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>DynAsesorias</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>ServletIndex</servlet-name>
    <servlet-class>dynAsesorias.controlador.Inicio</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ServletIndex</servlet-name>
  	<url-pattern>/inicio</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ServletCapacitacion</servlet-name>
    <servlet-class>dynAsesorias.controlador.CrearCapacitacion</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ServletCapacitacion</servlet-name>
  	<url-pattern>/capacitacion</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ServletContacto</servlet-name>
    <servlet-class>dynAsesorias.controlador.CrearContacto</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ServletContacto</servlet-name>
  	<url-pattern>/contacto</url-pattern>
  </servlet-mapping>  
  
  
	
  
  
  
  
</web-app>